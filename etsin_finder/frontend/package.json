  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "./__tests__/timezonedJSDOMEnvironment.js",
    "setupFilesAfterEnv": [
      "./__tests__/test-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/imageMock.js",
      "@/(.*)$": "<rootDir>/js/$1",
      "react-markdown": "<rootDir>/node_modules/react-markdown/react-markdown.min.js"
    },
    "testPathIgnorePatterns": [
      "./__tests__/test-setup.js",
      "./__tests__/test-helpers.js",
      "./__tests__/timezonedJSDOMEnvironment.js",
      "./__tests__/timezonedNodeEnvironment.js",
      "./__tests__/__testdata__",
      "./__tests__/__utils__",
      "./__tests__/components/componentTestHarness.jsx",
      "./__tests__/components/etsin/etsinHarness.jsx"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "globals": {
      "BUILD": "test"
    }
  },
  "prettier": {
    "function-paren-newline": false,
    "arrowParens": "avoid",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
