{
  "name": "etsin-finder",
  "version": "1.0.0",
  "description": "Research data finder",
  "main": "./static/index.html",
  "author": "Aaron Hakala",
  "repository": {
    "type": "git",
    "url": "https://github.com/CSCfi/etsin-finder"
  },
  "license": "ISC",
  "scripts": {
    "build": "webpack --mode production --config webpack.prod.js --node-env production",
    "dev-build": "webpack --config webpack.config.js --node-env development",
    "lint": "eslint js --ext .jsx,.js",
    "start": "webpack --progress --config webpack.config.js --watch --node-env development",
    "test": "jest",
    "coverage": "jest --collect-coverage --collect-coverage-from=\"js/**\"",
    "watch": "webpack --config webpack.config.js --watch --node-env development",
    "dev": "webpack serve --config webpack.dev.js --inline --node-env development --host 0.0.0.0 --port 8080"
  },
  "dependencies": {
