    "react-markdown": "^5.0.3",
    "react-modal": "^3.12.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^4.3.0",
    "react-transition-group": "^2.9.0",
    "react-translate-component": "0.15.1",
    "react-with-direction": "^1.3.0",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.1",
    "terraformer-wkt-parser": "1.2.0",
    "uuid": "^8.3.0",
    "webpack": "^5.27.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@axe-core/react": "^4.1.0",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^16.14.5",
    "axios-mock-adapter": "^1.17.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.10.7",
    "chai": "^4.2.0",
    "dotenv-webpack": "^7.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "history": "4.7.2",
    "jest": "^24.8.0",
    "jest-axe": "^4.1.0",
    "jest-cli": "^24.8.0",
    "postcss": "^8.2.8",
    "react-element-to-jsx-string": "^14.0.2",
    "react-refresh": "^0.10.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/test-setup.js"
    ],
    "moduleNameMapper": {
      "Components/(.*)$": "<rootDir>/js/components/$1",
      "Stores/(.*)$": "<rootDir>/js/stores/$1",
      "Utils/(.*)$": "<rootDir>/js/utils/$1",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/imageMock.js"
    },
    "testPathIgnorePatterns": [
      "./__tests__/test-setup.js",
      "./__tests__/__testdata__",
      "./__tests__/__utils__"
    ],
    "testURL": "http://localhost",
    "globals": {
      "BUILD": "test"
    }
  },
  "prettier": {
    "function-paren-newline": false,
    "arrowParens": "avoid",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
