{
  "env": {
    "development": {
      "plugins": [["babel-plugin-styled-components", { "minify": false }]]
    },
    "test": {
      "plugins": [
        ["babel-plugin-styled-components", { "minify": false }],
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": false }],
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-transform-runtime"]
      ],
      "presets": [
        [
          "@babel/preset-env",
          { "useBuiltIns": "entry", "corejs": 2, "targets": "> 0.25%, not dead" }
        ],
        "@babel/preset-react"
      ]
    }
  },
  "plugins": [
    ["babel-plugin-styled-components", { "displayName": false, "minify": false }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }],
    "@loadable/babel-plugin",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": "> 0.25%, not dead"
      }
    ],
    "@babel/preset-react"
  ]
}
